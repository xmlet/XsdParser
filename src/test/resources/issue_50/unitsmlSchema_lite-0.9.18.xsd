                            <xsd:documentation>deca: ten</xsd:documentation>
                        </xsd:annotation>
                    </xsd:enumeration>
                    <xsd:enumeration value="c">
                        <xsd:annotation>
                            <xsd:documentation>deci: tenth</xsd:documentation>
                        </xsd:annotation>
                    </xsd:enumeration>
                    <xsd:enumeration value="m">
