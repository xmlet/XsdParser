                    <xs:element ref="ocx:IBar" minOccurs="0"/>
                    <xs:element ref="ocx:LBarOF" minOccurs="0"/>
                    <xs:element ref="ocx:ZBar" minOccurs="0"/>
                    <xs:element ref="ocx:RoundBar" minOccurs="0"/>
                    <xs:element ref="ocx:LBar" minOccurs="0"/>
