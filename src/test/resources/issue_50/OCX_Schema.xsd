        <xs:annotation>
            <xs:documentation>Type definition of the  catalogue of rolled and welded cross-sections recognised by the Society.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ocx:DescriptionBase_T">
